{
  "extends": "tslint:recommended",
  "rules": {
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "interface-name": false,
    "no-console": false,
    "member-access": false,
    "align": false,
    "one-line": false,
    "arrow-parens": [false],
    "max-classes-per-file": [false]
  }
}